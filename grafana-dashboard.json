          "query": "data = from(bucket: \"vllm-metrics\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"prometheus\")\n  |> filter(fn: (r) => r._field == \"vllm:prompt_tokens_total\")\n  |> drop(columns: [\"engine\", \"model_name\", \"url\"])\n  |> derivative(unit: 1s, nonNegative: true)\n\nprompt_min = data\n  |> aggregateWindow(every: v.windowPeriod, fn: min)\n  |> rename(columns: {_value: \"prompt_min\"})\n  |> drop(columns: [\"_start\", \"_stop\", \"_field\"])\n  |> yield(name: \"prompt_min\")\n\nprompt_mean = data\n  |> aggregateWindow(every: v.windowPeriod, fn: mean)\n  |> rename(columns: {_value: \"prompt_mean\"})\n  |> drop(columns: [\"_start\", \"_stop\", \"_field\"])\n  |> yield(name: \"prompt_mean\")\n\nprompt_max = data\n  |> aggregateWindow(every: v.windowPeriod, fn: max)\n  |> rename(columns: {_value: \"prompt_max\"})\n  |> drop(columns: [\"_start\", \"_stop\", \"_field\"])\n  |> yield()\n",
